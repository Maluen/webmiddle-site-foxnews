{
  "name": "webmiddle-site-foxnews",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "ava",
    "test:watch": "npm run test -- --watch",
    "clean": "rimraf dist",
    "babel": "babel src --out-dir dist --source-maps inline",
    "build": "npm run clean && npm run babel",
    "build:watch": "npm run clean && npm run babel -- --watch",
    "prepublish": "npm run build"
  },
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "author": "Manuel Dell'Elce",
  "license": "MIT",
  "dependencies": {
    "webmiddle": "*",
    "webmiddle-service-arraymap": "*",
    "webmiddle-service-browser": "*",
    "webmiddle-service-cheerio-to-json": "*",
    "webmiddle-service-cheerio-to-virtual": "*",
    "webmiddle-service-http-request": "*",
    "webmiddle-service-jsonselect-to-json": "*",
    "webmiddle-service-jsonselect-to-virtual": "*",
    "webmiddle-service-parallel": "*",
    "webmiddle-service-pipe": "*",
    "webmiddle-service-virtual-to-json": "*",
    "webmiddle-service-resume": "*"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.6.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "proptypes": "^0.14.3",
    "rimraf": "^2.5.4",
    "virtual-element": "^1.2.0"
  }
}
